<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/main_navigation"
app:startDestination="@id/welcomeFragment">

<fragment
    android:id="@+id/welcomeFragment"
    android:name="com.example.number.presentation.WelcomeFragment"
    android:label="WelcomeFragment" >
    <action
        android:id="@+id/action_welcomeFragment_to_chooseLevelFragment2"
        app:destination="@id/chooseLevelFragment2" />
</fragment>
    <fragment
        android:id="@+id/chooseLevelFragment2"
        android:name="com.example.number.presentation.ChooseLevelFragment"
        android:label="fragment_choose_level"
        tools:layout="@layout/fragment_choose_level" >
        <action
            android:id="@+id/action_chooseLevelFragment2_to_gameFragment3"
            app:destination="@id/gameFragment3" />
    </fragment>
    <fragment
        android:id="@+id/gameFinishedFragment2"
        android:name="com.example.number.presentation.GameFinishedFragment"
        android:label="fragment_game_finished"
        tools:layout="@layout/fragment_game_finished" >
        <action
            android:id="@+id/action_gameFinishedFragment2_to_chooseLevelFragment2"
            app:destination="@id/chooseLevelFragment2" />
        <argument
            android:name="gameResult"
            app:argType="com.example.number.domain.entity.GameResult" />
    </fragment>
    <fragment
        android:id="@+id/gameFragment3"
        android:name="com.example.number.presentation.GameFragment"
        android:label="fragment_game"
        tools:layout="@layout/fragment_game" >
        <action
            android:id="@+id/action_gameFragment3_to_gameFinishedFragment2"
            app:destination="@id/gameFinishedFragment2"
            app:popUpTo="@id/gameFragment3"
            app:popUpToInclusive="true" />
        <argument
            android:name="level"
            app:argType="com.example.number.domain.entity.Level" />
    </fragment>
</navigation>